@{
  Layout = "_Layout";
}
<div class="container">
  @using Factory.Models;

  @model List<Factory.Models.License>;

  <h1>Repair Licenses</h1>
  <hr />

  @if (@Model.Count == 0)
  {
    <h3>There are currently no registered repair licenses.</h3>
  }
  <ul class="no-bullets">
    @foreach (License license in Model)
    {
        <li>@Html.ActionLink($"{license.LicenseType}", "Details", new { id = license.LicenseId})</li><br>
    }
  </ul>
  <br><p>@Html.ActionLink("Log New Repair License", "Create")</p>
  <br><p>@Html.ActionLink("Homepage", "Index", "Home")</p>
</div>